<h1>Create a New Spell-Cat</h1>
  
<%= form_for @cat, url: user_cats_path, method: :post do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :age %>
  <%= f.number_field :age %><br>
  <%= f.label :type_of %>
  <%= f.text_field :type_of %><br>
  <%= f.submit "Create Cat" %>
<% end %>
